{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\n",
          "type": "text",
          "style": "powerline"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0e7 ",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#ffdd81",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .UserName }} @ {{ .HostName }} ",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#bababa",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": "\\",
            "style": "full"
          },
          "template": " {{ .Path }} ",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#5ac5f7",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "background": "#30ff4b",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#30ff4b{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#00aa00{{ end }}",
            "{{ if gt .Ahead 0 }}#ff9999{{ end }}",
            "{{ if gt .Behind 0 }}#ff6666{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#a384ff",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue20f ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#ff4d4d",
          "type": "text",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "‚ùØ",
          "foreground": "#007ACC",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
